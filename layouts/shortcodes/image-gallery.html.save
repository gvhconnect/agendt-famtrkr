<style>
    .image-gallery {overflow: auto; margin-left: -1%!important;}
    .image-gallery li {float: left; display: block; margin: 0 0 1% 1%; width: 19%;}
    .image-gallery li a {text-align: center; text-decoration: none!important; color: #777;}
    .image-gallery li a span {display: block; text-overflow: ellipsis; overflow: hidden; white-space: nowrap; padding: 3px 0;}
    .image-gallery li a img {width: 100%; display: block;}
  </style>

# Set the bucket name and folder path
bucket="0-go4wd-dam"
folder="acct-01/user-02"


	#objects = []
	#bucket_contents = []
	#for key in s3.list_objects(Bucket= '0-go4wd-dam', Prefix = 'acct-01/user-02/')

# Use the aws command to list the objects in the folder
objects=$(aws s3 ls s3://$bucket/$folder/)

# Loop through the objects and generate the shortcodes for object in $objects; do
  # Extract the file name from the object
  filename=$(echo $object | awk '{print $4}')
  # Generate the image shortcode
  image_shortcode="{{< figure src=\"https://s3.amazonaws.com/$bucket/$folder/$filename\" >}}"
  # Add the shortcode to the Markdown file


  
  {{ $dir := getJSON "https://d7okgpmgzkdc1.cloudfront.net/acct-01/user-02/object_list_filtered.json" }}
  <ul class="image-gallery">
  {{ range (readDir (print "/static")) $dir }}
  {{- $image := resources.Get (printf "%s/%s" $dir .objects) -}}
  {{- $imageurl := printf "%s/%s" $dir .objects -}}
  {{- $imagetitle := index (split .objects ".") 0 -}}
    <li>
      <a href="{{ ($image.Fit "1600x1600 q50").Permalink }}" title="{{ $imagetitle }}" class="lightbox-image">
        <img src="{{ ($image.Fill "300x300 q50").Permalink }}" alt="{{ $imagetitle }}" title="{{ $imagetitle }}">
        <span>{{ $imagetitle }}</span>
      </a>
    </li>
  
  {{ end }}
  </ul>
  
